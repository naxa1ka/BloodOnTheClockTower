}

:root {
    --white-transparent: rgba(255, 255, 255, 0.1);
    --white-transparent-half: rgba(255, 255, 255, 0.05);
    --white-transparent-hover: rgba(255, 255, 255, 0.001);
    --white-transparent-active: rgba(255, 255, 255, 0.02);
}

:root {
    --light-black: rgb(35, 35, 35);
    --dark-black: rgb(28, 28, 28);
    --deep-black: rgb(14, 14, 14);
    --highlight-gray: rgb(142, 142, 147);
    --light-gray: rgb(99, 99, 102);
    --mid-gray: rgb(57, 57, 60);
    --dark-gray: rgb(49, 49, 52);
    --deep-gray: rgb(44, 44, 46);
    --deep-red: rgb(199, 59, 48);
    --deep-blue: rgb(10, 99, 212);
    --deep-green: rgb(33, 170, 70);
    --deep-orange: rgb(189, 114, 10);
    --transparent: rgba(0, 0, 0, 0);
    --gray-transparent: rgba(142, 142, 147, 0.05);
    --black-transparent: rgba(18, 18, 18, 0.9);
    --red-transparent: rgba(255, 59, 48, 0.05);
    --blue-transparent: rgba(10, 132, 255, 0.05);
    --green-transparent: rgba(48, 209, 88, 0.05);
    --orange-transparent: rgba(255, 149, 0, 0.05);
    --brown-transparent: rgba(172, 142, 104, 0.05);
    --indigo-transparent: rgba(94, 92, 230, 0.05);
    --red-error: rgba(255, 59, 48, 0.25);
    --red-selection: rgba(255, 59, 48, 0.4);
    --orange-selection: rgba(255, 149, 0, 0.4);
}

:root {
    --base-size-extra-small: 40px;
    --base-size-small: 48px;
    --base-size-middle: 64px;
    --base-size-large: 88px;
    --base-size-extra-large: 128px;
    --base-size-extra-large-double: 256px;
    --base-icon-size-extra-small: 24px;
    --base-icon-size-small: 28px;
    --base-icon-size-middle: 32px;
    --base-icon-size-large: 36px;
    --base-icon-size-extra-large: 48px;
    --base-border-radius: 12px;
    --base-border-radius-half: 8px;
    --base-font-size-extra-small: 18px;
    --base-font-size-small: 22px;
    --base-font-size-middle: 38px;
    --base-font-size-large: 42px;
    --base-margin-double: 32px;
    --base-margin: 16px;
    --base-margin-half: 8px;
    --base-margin-quarter: 4px;
    --base-padding-double: 32px;
    --base-padding: 16px;
    --base-padding-half: 8px;
    --base-padding-quarter: 4px;
    --base-line-height: 1px;
    --base-panel-size-middle: 416px;
    --base-panel-size-large: 608px;
    --base-opacity-disabled: 0.3;
}

:root {
    --animation-transition-duration: 0.25s;
    --animation-transition-delay: 0s;
    --animation-transition-timing-function: ease-in-out-sine;
}


:root {
    --button-height: var(--base-size-middle);
    --button-width: var(--base-size-middle);
    --button-border-radius: var(--base-border-radius);
    --button-font-size: var(--base-font-size-middle);
    --button-font-definition: var(--font-medium);
    --button-transition-duration: var(--animation-transition-duration);
    --button-keyboard-size: 64px;
}

:root {
    --input-field-error-cursor-color: var(--red);
    --input-field-selection-error-color: var(--red-selection);
    --input-field-cursor-color: var(--orange);
    --input-field-selection-color: var(--orange-selection);
    --input-field-label-color: var(--highlight-gray);
    --input-field-label-background-color-x: var(--deep-red);
    --input-field-label-background-color-y: var(--deep-green);
    --input-field-label-background-color-z: var(--deep-blue);
    --input-field-input-background-color: var(--gray);
    --input-field-input-border-color: var(--light-gray);
    --input-field-input-hover-border-color: var(--highlight-gray);
    --input-field-input-focus-border-color: var(--orange);
    --input-field-label-error-color: var(--red);
    --input-field-input-error-border-color: var(--red);
    --input-field-input-error-background-color: var(--red-error);
    --input-field-input-error-color: var(--white);
    --input-filed-input-color: var(--white);
    --input-filed-input-preview-color: var(--highlight-gray);
    --input-filed-input-keyboard-checked-focus-color: var(--white);
    --input-field-input-font-size: var(--base-font-size-middle);
    --input-field-label-font-size: var(--base-font-size-middle);
    --input-field-label-font-definition: var(--font-regular);
    --input-field-height: var(--base-size-middle);
    --input-field-label-margin-bottom: var(--base-margin-half);
    --input-filed-input-border-radius: var(--base-border-radius-half);
    --input-filed-input-padding-left: var(--base-padding-half);
    --input-filed-input-padding-left-half: var(--base-padding-quarter);
    --input-filed-input-border-width: var(--base-line-height);
    --input-field-disabled-opacity: var(--base-opacity-disabled);
}



