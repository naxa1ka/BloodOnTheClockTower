    --eggshell: rgb(245, 243, 230);
    --cyan-hover: rgb(115, 214, 255);
    --cyan-active: rgb(90, 189, 229);
    --blue: rgb(10, 132, 255);
    --blue-hover: rgb(34, 144, 255);
